{
  "name": "twinql",
  "version": "0.11.1",
  "description": "A graph query language for the semantic web",
  "main": "lib/index.js",
  "files": [
    "lib",
    "build"
  ],
  "scripts": {
    "start": "npm run demo",
    "lint": "standard --verbose 'src/**/*.js'",
    "mocha": "mocha --require babel-register test/setup.js 'src/**/*.js'",
    "pretest": "npm run lint",
    "test": "cross-env NODE_ENV=test nyc npm run mocha",
    "test:dev": "npm run mocha -- --watch --growl",
    "docs": "jsdoc src -r -c conf.json -d docs",
    "demo": "http-server . -o",
    "build": "npm run build:lib && npm run build:umd",
    "build:lib": "babel src -d lib",
    "build:dev": "webpack --config webpack.dev.config.js --watch",
    "build:umd": "webpack --config webpack.prod.config.js",
    "coverage:report": "nyc report",
    "coverage:coveralls": "nyc report --reporter=text-lcov | coveralls",
    "postversion": "git push --follow-tags",
    "prepublish": "npm run build"
  },
  "author": "Daniel Friedman",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-immutable": "^1.6.0",
    "coveralls": "^2.13.0",
    "cross-env": "^4.0.0",
    "http-server": "^0.9.0",
    "jsdoc": "^3.4.3",
    "jsdoc-strip-async-await": "^0.1.0",
    "mocha": "^3.2.0",
    "nock": "^9.0.13",
    "nyc": "^10.2.0",
    "proxyquire": "^1.7.11",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0",
    "standard": "^10.0.2",
    "webpack": "^2.3.2"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "immutable": "^4.0.0-rc.2",
    "isomorphic-fetch": "^2.2.1",
    "n3": "^0.9.1",
    "valid-url": "^1.0.9"
  },
  "nyc": {
    "reporter": [
      "html",
      "text"
    ],
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/*.spec.js"
    ]
  },
  "standard": {
    "ignore": [
      "src/**/*.spec.js"
    ]
  }
}
