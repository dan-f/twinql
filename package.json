{
  "name": "twinql",
  "version": "0.1.0",
  "description": "A graph query language for the semantic web",
  "main": "lib/index.js",
  "files": [
    "lib",
    "build"
  ],
  "scripts": {
    "postinstall": "npm run build",
    "start": "npm run demo",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docs": "jsdoc src -r -c conf.json -d docs",
    "demo": "http-server -o",
    "build": "npm run build:lib && npm run build:umd",
    "build:lib": "babel src -d lib",
    "build:dev": "webpack --config webpack.dev.config.js --watch",
    "build:umd": "webpack --config webpack.prod.config.js",
    "postversion": "git push --follow-tags",
    "prepublish": "npm run build"
  },
  "author": "Daniel Friedman",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "http-server": "^0.9.0",
    "jsdoc": "^3.4.3",
    "jsdoc-strip-async-await": "^0.1.0",
    "webpack": "^2.3.2"
  },
  "dependencies": {
    "immutable": "^4.0.0-rc.1",
    "isomorphic-fetch": "^2.2.1",
    "n3": "^0.9.1",
    "valid-url": "^1.0.9"
  }
}
